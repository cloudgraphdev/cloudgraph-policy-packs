    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@autocloud/eslint-config": "^0.1.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/gitlab": "^7.0.3",
    "@semantic-release/npm": "^8.0.0",
    "@types/jest": "^26.0.24",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.4",
